<Group labelreference="L377" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">str_num := proc(a, b):
    if a &gt; b then
        cat(convert(a, string), &quot; &gt; &quot;, convert(b, string));
    elif a = b then
        cat(convert(a, string), &quot; = &quot;, convert(b, string));
