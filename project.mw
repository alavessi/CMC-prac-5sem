<Group hide-input="false" labelreference="L7" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">get_color := proc(i, left, right, cur):
    piecewise(
        i = cur, red,
        i = left, blue,
        i = right, COLOUR(RGB, 1, 0.54, 0),
        i = cur, red,
        (left &lt;= i and right &gt;= i) or left = -1, white,
        COLOUR(RGB, 0.6, 0.6, 0.6)
    )
end proc:</Text-field>
