<Group labelreference="L211" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Draw_final := proc(cur_arr, ind, col):
    local n, i, gist;
    n := ArrayTools[Size](cur_arr)[2];
